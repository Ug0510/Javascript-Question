// program to create a new string without the first and last characters of a given string.  

function first_last_remover(str)
{
    if(str.length < 2)
    {
        return false;
    }

    return str.substring(1,str.length - 1);
}

console.log(first_last_remover('yo'));
console.log(first_last_remover('a'));
console.log(first_last_remover('nice'));