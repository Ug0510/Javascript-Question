// program to test whether a string ends with "Script". The string length must be greater than or equal to 6.

function ends_with(str)
{
    if(str.length < 6)
    {
        return false;
    }

    if(str.slice(-6) == 'Script')
    {
        return true;
    }
    return false;
}

console.log(ends_with("Java Scripts"));
console.log(ends_with("JavaScript"))