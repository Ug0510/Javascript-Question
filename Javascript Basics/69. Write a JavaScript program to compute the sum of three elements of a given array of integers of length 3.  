// program to compute the sum of three elements of a given array of integers of length 3.  

function sumArray(arr)
{
    if(arr.length == 3)
    {
        let sum = 0;
        for(let i of arr)
        {
            sum += i;
        }
        return sum;
    }
    return;
}

console.log(sumArray([1,3,5]));
console.log(sumArray([23,12,4]));