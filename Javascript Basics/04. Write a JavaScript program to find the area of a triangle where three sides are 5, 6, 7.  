// value of sides are given 5,6 and 7
let s1 = 5;
let s2 = 6;
let s3 = 7;

// first calculating semi-perimeter of triangle ABC
let s = (s1 + s2 + s3)/2;

//calculating area of triangle
let area = Math.sqrt(s * (s - s1) * (s - s2) * (s - s3));

console.log("Area of triangle with side ",s1+","+s2+","+s3+" is",area);
