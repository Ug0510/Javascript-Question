// program to check if number is multiple of 3 or 7

const num = parseInt(prompt("Enter a number"));

if(num % 3 == 0 && num % 7 == 0)
{
    console.log("Multiple of both 3 and 7");
}
else if(num % 3 == 0)
{
    console.log("Multiple of 3 only");
}
else if(num % 7 == 0)
{
    console.log("Multiple of 7 only");
}
else
{
    console.log("Neither a muliple of 3 nor 7");
}