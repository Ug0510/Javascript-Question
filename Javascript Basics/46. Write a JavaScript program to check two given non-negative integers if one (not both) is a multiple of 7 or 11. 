// program to check two given non-negative integers if one (not both) is a multiple of 7 or 11. 

function checkMultipleOf7Or11(n)
{
    if(n % 7 == 0 || n % 11 == 0)
        return true;
    else
        return false; 
}
function valCheck(x , y)
{
    if( x > 0 && y > 0)
    {
        if(checkMultipleOf7Or11(x) && !checkMultipleOf7Or11(y))
        {
            return true;
        }
        else if(checkMultipleOf7Or11(x) && !checkMultipleOf7Or11(y))
        {
            return true;
        }
    }
    return false;
}

console.log(valCheck(14, 21));
console.log(valCheck(14, 20));
console.log(valCheck(16, 20));