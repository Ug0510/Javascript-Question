// program to check from two given integers whether one of them is 8 or their sum or difference is 8

function find8(n1, n2)
{
    if((n1 == 8 || n2 == 8) || (n1+n2 == 8) || (Math.abs(n1-n2) == 8)) 
    {
        return true;
    }
    return false;
}
console.log(find8(4,4));
console.log(find8(8,1));
console.log(find8(1,8));
console.log(find8(15,7));