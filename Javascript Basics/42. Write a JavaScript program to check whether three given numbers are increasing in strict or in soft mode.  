// program to check whether three given numbers are increasing in strict or in soft mode.  

function checkStrictMode(a,b,c)
{
    if(a < b && b < c)
    {
        return "strict mode";
    }
    else if(c > b)
    {
        return "soft mode";
    }
    else{
        return "Undefined";
    }
}

console.log(checkStrictMode(22,22,31));
console.log(checkStrictMode(15,27,56));
console.log(checkStrictMode(24, 22, 31));